})




local Tab = Window:CreateTab("main", 10590477450)


local Button = Tab:CreateButton({
	Name = "infstamina",
	Callback = function()
		while wait(0.4) do
		game.Players.LocalPlayer.Character.Stats.Stamina.Value = 100
		end
    end,
})
